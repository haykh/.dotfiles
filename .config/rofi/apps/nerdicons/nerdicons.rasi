window {
  width: 50%;
}

textbox-prompt {
  str: @symbol-nerdicons;
  color: @col-nerdicons;
}

entry {
  placeholder: @placeholder-nerdicons;
}

listview {
  require-input: true;
  columns: 5;
  lines: 6;
  spacing: 0.25em;
}

element-text {
  font: "Cantarell 14";
}

element-icon {
  horizontal-align: 0.65;
  size: 4em;
}

element selected {
  background-color: #2D5CAD;
  border: 0px solid;
}
