#%Module1.0#####################################################################
# 
# gcc-11.4
#
################################################################################
proc ModulesHelp { } {
  global version
  puts stderr "\tGCC-11.4\n"
}

module-whatis   "Loads settings for the gcc-11.4"
set             basedir         /usr
prepend-path    PATH            /usr/bin
prepend-path    MANPATH         /usr/share/man
prepend-path    INFOPATH        /usr/share/info
prepend-path    PKG_CONFIG_PATH /usr/lib/pkgconfig
